{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.16.0",
    "generators": {
      "lgg-python": {
        "generatorName": "python",
        "output": "src/python/lgg",
        "inputSpec": "../../apps/ls-api/docs/openapi.json",
        "additionalProperties": {
          "packageName": "lgg_client",
          "projectName": "lgg-client",
          "packageVersion": "0.0.0"
        },
        "enablePostProcessFile": true,
        "removeOperationIdPrefix": true
      },
      "simulator-python": {
        "generatorName": "python",
        "output": "src/python/simulator",
        "inputSpec": "../../apps/simulator/docs/openapi.json",
        "additionalProperties": {
          "packageName": "simulator_client",
          "projectName": "simulator-client",
          "packageVersion": "0.0.0"
        },
        "enablePostProcessFile": true,
        "removeOperationIdPrefix": true
      }
    }
  }
}
